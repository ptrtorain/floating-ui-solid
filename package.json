{
  "name": "floating-ui-solid",
  "version": "1.0.17",
  "type": "module",
  "main": "./dist/index.js", 
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts --treeshake",
    "build:prod": "tsup src/index.ts --format esm --dts --treeshake --minify",
    "prepublishOnly": "npm run build:prod",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "biome lint"
  },
  "keywords": [
    "solid",
    "floating-ui",
    "solidjs",
    "positioning",
    "tooltip"
  ],
  "author": "rainyEra",
  "repository": {
    "type": "git",
    "url": "https://github.com/rainyEra/floating-ui-solid"
  },
  "license": "MIT",
  "description": "Floating UI Primitives for SolidJS",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "solid-js": "^1.8.18",
    "tsup": "^8.1.0",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.7"
  }
}
